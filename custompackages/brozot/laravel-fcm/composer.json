{
    "name": "brozot/laravel-fcm",
    "description": "Laravel / Lumen package for Firebase Cloud Messaging ",
    "keywords": ["laravel", "lumen", "firebase", "notification", "push", "fcm", "firebase cloud messaging"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Brosy",
            "email": "nicolas.brosy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2", 
        "guzzlehttp/guzzle": "^6.3"
      
       

     
    },
    "require-dev": {
        "mockery/mockery" : "0.9.*",
        "phpunit/phpunit" : "^8.5",
        "satooshi/php-coveralls": "^2.2",
        "laravel/laravel": "7.0.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelFCM\\": "src/",
            "LaravelFCM\\Mocks\\": "tests/mocks"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFCM\\FCMServiceProvider"
            ],
            "aliases": {
                "FCM": "LaravelFCM\\Facades\\FCM",
                "FCMGroup": "LaravelFCM\\Facades\\FCMGroup"
            }
        }
    }
}
